			}
			if(newcar >= rep[0] && newcar <= rep[1] || IsPlayerInRangeOfPoint(playerid,6.0,1824.3669,-1283.0090,120.2656))
			{
				GetPlayerName(playerid, sendername, sizeof(sendername));
		 		format(string, sizeof(string), "{FFFFFF}*CNN* %s: {AA8C00}%s {FFFFFF}*", sendername, result);
				OOCNews(COLOR_NEWS,string);
