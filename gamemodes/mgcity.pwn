CMD:heal(playerid, params[])
{
	new string[256],tmp[24],moneys,giveplayerid,sendername[MAX_PLAYER_NAME],giveplayer[MAX_PLAYER_NAME];
	new location = PlayerInfo[playerid][pLocal];
    if (!sscanf(params, "s[24]i", tmp,moneys))
 	{
 	    giveplayerid = ReturnUser(tmp);
		GetPlayerName(playerid, sendername, sizeof(sendername));
		if(moneys < 1 || moneys > 1000) return SendClientMessage(playerid, COLOR_GREY, "   Цена лечения не может быть менее 1 или больше 1000 !");
		if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_GRAD1, "   Вы не можете лечить себя!");
		if(IsPlayerConnected(giveplayerid))
		{
		    if(giveplayerid != INVALID_PLAYER_ID)
		    {
				GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
				GetPlayerName(playerid, sendername, sizeof(sendername));
				new giveambu = GetPlayerVehicleID(giveplayerid);
				new playambu = GetPlayerVehicleID(playerid);
				if(gTeam[playerid] == TEAM_CYAN||PlayerInfo[playerid][pMember]==4||PlayerInfo[playerid][pLeader]==4)//model
				{
					if (IsAnAmbulance(playambu) && playambu == giveambu)
					{
						new Float:tempheal;
						GetPlayerHealth(giveplayerid,tempheal);
						if(tempheal >= 100.0) return SendClientMessage(playerid, TEAM_GREEN_COLOR,"   Он здоров.");
						format(string, sizeof(string), "~y~lechenie ~n~~w~%s~n~~g~$%d", giveplayer,moneys);
						GameTextForPlayer(playerid, string, 5000, 1);
					    SetPVarInt(giveplayerid, "HealOffer", playerid);
						SetPVarInt(giveplayerid, "HealMoney",moneys);
						format(string, sizeof(string), "* Вы предложили %s вылечится за $%d", giveplayer, moneys);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						format(string, sizeof(string), "* Медик %s Предложил вам вылечится за $%d", sendername, moneys);
						SendClientMessage(giveplayerid, COLOR_LIGHTBLUE, string);
						SendClientMessage(giveplayerid, COLOR_LIGHTBLUE, "*Введите /ac heal, чтобы вылечиться.");
						if(STDPlayer[giveplayerid] > 0)
			            {
							STDPlayer[giveplayerid] = 0;
							SendClientMessage(giveplayerid, COLOR_LIGHTBLUE, "*Больше вы не заражены STD благодаря медикам !");
						}
					}
					else return SendClientMessage(playerid, COLOR_GRAD1, "  Один из вас не находится в санитарной машине/вертолете !");
				}
				else return SendClientMessage(playerid, COLOR_GRAD1, "   Не имеете права использовать эту команду !");
			}
		}
		else return SendClientMessage(playerid, COLOR_GRAD1, "   Этот игрок оффлайн");
	}
	else
	{
	    if(location != 9999)
		{
            GetPlayerName(playerid, sendername, sizeof(sendername));
			if (GetPlayerVirtualWorld(playerid) >= 50)
			{
				if(HouseInfo[location][hArm] == 1 && IsACop(playerid))
				{
					format(string, sizeof(string), "* %s надевает бронежилет.", sendername);
					ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					SetArm(playerid, 100.0);
				}
				else SendClientMessage(playerid, COLOR_GRAD2,"В этом месте нет апгрейда брони.");
				if(HouseInfo[location][hHel] == 1)
				{
					new Float:tempheal;
					GetPlayerHealth(playerid,tempheal);
					if(tempheal < 100.0)
					{
						SetPlayerHealth(playerid,100.0);
						PlayerPlaySound(playerid, 1150, 0.0, 0.0, 0.0);
						format(string, sizeof(string), "Вы были вылечены до 100 процентов.");
						SendClientMessage(playerid, TEAM_GREEN_COLOR,string);
					}
					else SendClientMessage(playerid, TEAM_GREEN_COLOR,"Вы уже здоровы на 100 процентов.");
				}
				else SendClientMessage(playerid, TEAM_GREEN_COLOR," В этом месте нет апгрейда аптечки.");
				return 1;
			}
			else if(location == 101)//Restaurant
			{
			  	new Float:tempheal;
				GetPlayerHealth(playerid,tempheal);
				if(tempheal < 100.0)
				{
					SetPlayerHealth(playerid,100.0);
					PlayerPlaySound(playerid, 1150, 0.0, 0.0, 0.0);
					SendClientMessage(playerid, TEAM_GREEN_COLOR,"Вы были вылечены на 100 процентов");
				}
				else SendClientMessage(playerid, TEAM_GREEN_COLOR," Вы уже здоровы на 100 процентов.");
			}
			else if(location == 102 && IsACop(playerid))//Police Armoury
			{
			    SetPlayerHealth(playerid,100.0);
			    format(string, sizeof(string), "* %s надевает броню.", sendername);
				ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
				TogglePlayerControllable(playerid, 0);
				GetPlayerPos(playerid, Unspec[playerid][sPx], Unspec[playerid][sPy], Unspec[playerid][sPz]);
				Unspec[playerid][sPint] = PlayerInfo[playerid][pInt];
				Unspec[playerid][sLocal] = PlayerInfo[playerid][pLocal];
				SetPlayerPos(playerid,1527.5,-12.1,1002.0);
				PlayerInfo[playerid][pLocal] = 9999;
				SetPlayerInterior(playerid,99);
				Spectate[playerid] = 557;
				SetArm(playerid, 100);
			}
		}
		else SendClientMessage(playerid, COLOR_GRAD2, "*Используйте: /heal [id игрока/часть ника] [цена]");
	}
	return 1;
}
