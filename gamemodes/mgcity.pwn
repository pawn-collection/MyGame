stock AntiMoneyCheat(playerid)
{
	new clientmoney = oGetPlayerMoney(playerid);
	if(clientmoney < PlayerInfo[playerid][pMoney]) return oSetPlayerMoney(playerid,clientmoney);
	if(clientmoney > PlayerInfo[playerid][pMoney]) return oSetPlayerMoney(playerid,PlayerInfo[playerid][pMoney]);
	return 1;
}

stock oGivePlayerMoney(playerid, money)
{
	PlayerInfo[playerid][pMoney] += money;
	return oSetPlayerMoney(playerid, PlayerInfo[playerid][pMoney]);
}

stock oSetPlayerMoney(playerid, money)
{
	PlayerInfo[playerid][pMoney] = money;
	ResetPlayerMoney(playerid);
	return GivePlayerMoney(playerid,money);
}

