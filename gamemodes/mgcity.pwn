public Lotto(number)
{
	new JackpotFallen = 0;
	new string[256];
	format(string, sizeof(string), "Лотто [RP]MyGame: {FF9000}Сегодня выиграшный номер  {FFFFFF}%d.", number);
    OOCOff(COLOR_WHITE, string);
    foreach (Player, i)
	{
	    if(PlayerInfo[i][pLottoNr] > 0)
	    {
		    if(PlayerInfo[i][pLottoNr] == number)
		    {
		        JackpotFallen = 1;
				format(string, sizeof(string), "Лотто [RP]MyGame: {FF0000}%s выигрывает Джекпот в размере {09bd00}$%d .", GetName(i), Jackpot);
				OOCOff(COLOR_WHITE, string);
				format(string, sizeof(string), "{FF9000}* Вы выиграли {09bd00}$%d {FF9000}в лотерею.", Jackpot);
				SendClientMessage(i, COLOR_YELLOW, string);
				oGivePlayerMoney(i, Jackpot);
		    }
		    else
		    {
		        SendClientMessage(i, COLOR_LIGHTBLUE, "{FF9000}* {FF0000}Вы не выйграли сейчас, может повезёт потом.");
		    }
		}
		PlayerInfo[i][pLottoNr] = 0;
	}
	if(JackpotFallen)
	{
	    new rand = random(6050); rand += 20000;
	    Jackpot = rand;
	    SaveStuff();
	    format(string, sizeof(string), "Лотто [RP]MyGame: {FF9000}Назначен новый Джекпот: {09bd00}$%d.", Jackpot);
		OOCOff(COLOR_WHITE, string);
	}
	else
	{
	    new rand = random(150); rand += 10000;
	    Jackpot += rand;
	    SaveStuff();
	    format(string, sizeof(string), "Лотто [RP]MyGame: {FF9000}Назначен новый Джекпот:  {09bd00}$%d.", Jackpot);
		OOCOff(COLOR_WHITE, string);
	}
	return 1;
}


