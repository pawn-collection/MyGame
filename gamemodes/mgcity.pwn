			ACGivePlayerWeapon(playerid, 5, 1);
			ACGivePlayerWeapon(playerid, 24, 30);
			return 1;
		}
		if(PlayerInfo[playerid][pMember] == 24 || PlayerInfo[playerid][pLeader] == 24)
		{
			ACGivePlayerWeapon(playerid, 8, 1);
           	ACGivePlayerWeapon(playerid, 24, 30);
			return 1;
		}
		if(PlayerInfo[playerid][pMember] == 25 || PlayerInfo[playerid][pLeader] == 25)
		{
			ACGivePlayerWeapon(playerid, 5, 1);
			ACGivePlayerWeapon(playerid, 24, 30);
			return 1;
		}
        if(PlayerInfo[playerid][pMember] == 9 || PlayerInfo[playerid][pLeader] == 9)//report
		{
   			ACGivePlayerWeapon(playerid, 43, 200);
   			ACGivePlayerWeapon(playerid, 24, 30);
			return 1;
		}
	}
	else
	{
	    if (PlayerInfo[playerid][pGun1] > 0)
		{
			ACGivePlayerWeapon(playerid, PlayerInfo[playerid][pGun1], PlayerInfo[playerid][pAmmo1]);
			PlayerInfo[playerid][pGun1] = 0; PlayerInfo[playerid][pAmmo1] = 0;
			return 1;
		}
		if (PlayerInfo[playerid][pGun2] > 0)
		{
			ACGivePlayerWeapon(playerid, PlayerInfo[playerid][pGun2], PlayerInfo[playerid][pAmmo2]);
			PlayerInfo[playerid][pGun2] = 0; PlayerInfo[playerid][pAmmo2] = 0;
			return 1;
		}
	}
	return 1;
}

