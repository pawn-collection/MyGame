					        case 11: ftext = "Лицензеры";
					        case 12: ftext = "Русскую мафию";
					        case 13: ftext = "Groove Street";
					        case 14: ftext = "Ballas Gang";
					        case 15: ftext = "Vagos Gang";
					        case 16: ftext = "El Coronos";
					        case 17: ftext = "Rifa Gang";
					        case 18: ftext = "Bloods Gang";
					        case 19: ftext = "Street Racers";
					        case 20: ftext = "Ruff Ryders";
							case 21: ftext = "S.W.A.T.";
							case 22: ftext = "S.F.P.D.";
                            case 23: ftext = "Crips Gang";
							case 24: ftext = "The Triads Mafia";
							case 25: ftext = "RUS ОПГ";
		     			}
