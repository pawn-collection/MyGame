	new giveplayerid;
    if (!sscanf(params, "i", giveplayerid))
	{
		if(PlayerInfo[playerid][pAdmin] >= 2000)
		{
		    if(IsPlayerConnected(giveplayerid))
		    {
				SetPlayerHealth(giveplayerid, 10);
				new Float:boomx, Float:boomy, Float:boomz;
				GetPlayerPos(giveplayerid,boomx, boomy, boomz);
				CreateExplosion(boomx, boomy , boomz, 7, 10);
			}
			else SendClientMessage(playerid, COLOR_RED, "Не активный игрок.");
		}
		else SendClientMessage(playerid, COLOR_RED, " У тебя нет прав на использование этой команды!");
	}
	else SendClientMessage(playerid, COLOR_GREY, " /explode [IDигрока]");
	return 1;
}
CMD:rac(playerid, params[]) return cmd_respawn(playerid, params);
CMD:respawn(playerid, params[])
{
	new string[256],sendername[MAX_PLAYER_NAME];
 	if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessage(playerid, COLOR_GRAD1, "   Вы не админ");
	new bool:unwanted[CAR_AMOUNT];
	foreach(Player,player)
	{
       	if(IsPlayerInAnyVehicle(player)) { unwanted[GetPlayerVehicleID(player)]=true; }
       	if(IsTrailerAttachedToVehicle(GetPlayerVehicleID(player))) { unwanted[GetVehicleTrailer(GetPlayerVehicleID(player))]=true; }
	}
	for(new car = 1; car < 1129; car++)
	{
		if(!unwanted[car]) SetVehicleToRespawn(car);
	}
	GetPlayerName(playerid, sendername, sizeof(sendername));
	format(string, sizeof(string), "Админ %s зареспавнил все автомобили.", sendername);
	SendClientMessageToAll(COLOR_WHITE,string);
	return 1;
}
CMD:knockout(playerid, params[])
{
	new giveplayerid,string[256],sendername[MAX_PLAYER_NAME],giveplayername[MAX_PLAYER_NAME];
    if (!sscanf(params, "i", giveplayerid))
	{
	    if(gPlayerLogged[playerid] == 0) return SendClientMessage(playerid, COLOR_GREY, " Сначала нужно залогиниться.");
	    if(PlayerInfo[playerid][pLevel] < 5) return SendClientMessage(playerid, COLOR_GREY, " Тебе нужно быть 5 уровня, чтобы использовать эту команду.");
	    if(KnockedDown[playerid] == 1 || PlayerCuffed[playerid] > 0) return SendClientMessage(playerid, COLOR_GREY, " Тебя побили, ты не можешь сделать это.");
		if(IsPlayerConnected(giveplayerid))
		{
  			if(giveplayerid != INVALID_PLAYER_ID)
		    {
		        if (ProxDetectorS(1.0, playerid, giveplayerid))
		        {
          			if(KnockedDown[giveplayerid] != 1 || PlayerCuffed[giveplayerid] > 0)
		        	{
		        	    if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_GREY, "   Ты не можешь ударить самого себя.");
		        	    if(PlayerInfo[giveplayerid][pSex] == 2) return SendClientMessage(playerid, COLOR_GREY, " Подлец! Захотел ударить женщину?");
		        	    if(IsPlayerInAnyVehicle(giveplayerid)) { return 1; }
		            	GetPlayerName(playerid, sendername, sizeof(sendername));
		            	GetPlayerName(giveplayerid, giveplayername, sizeof(giveplayername));
		            	new rands = random(2)+1;
						format(string, sizeof(string), "* %s собираеться ударить %s и вырубить", sendername, giveplayername);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
                        ApplyAnimation(playerid,"FIGHT_C","FightC_3",4.1,0,1,1,1,1);
						if(rands == 1)
						{
						    TogglePlayerControllable(giveplayerid, 0);
			   				ApplyAnimation(giveplayerid, "PED", "KO_shot_face", 4.1, 0, 1, 1, 1, 1);
						    SetTimerEx("ClearKnock", 20000, false, "i", giveplayerid);
			   				ApplyAnimation(playerid,"FIGHT_C","FightC_3",4.1,0,1,1,1,1);
							KnockedDown[giveplayerid] = 1;
							format(string, sizeof(string), "* %s сбил с ног %s, ударом по голове.", sendername, giveplayername);
							ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
						}
						else
						{
						    format(string, sizeof(string), "* %s уворачиваеться от удара  %s .", giveplayername, sendername);
                            ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
						}
						return 1;
					}
					else SendClientMessage(playerid, COLOR_GREY, " Этот человек уже в нокауте.");
     			}
		        else SendClientMessage(playerid, COLOR_GREY, " Человек не рядом с тобой.");
		    }
		}
	}
	else SendClientMessage(playerid, COLOR_WHITE, " Подсказка: /knockout [IDигрока]");
	return 1;
}
