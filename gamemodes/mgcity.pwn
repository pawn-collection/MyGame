CMD:refill(playerid, params[])
{
	new playa,money,giveplayer[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME],string[256];
	if (!sscanf(params, "ii", playa,money))
 	{
  		if(PlayerInfo[playerid][pJob] != 7) return SendClientMessage(playerid, COLOR_GREY, "Вы не Механик!");
		if(money < 1 || money > 99999) return SendClientMessage(playerid, COLOR_GREY, "Цена должна быть не ниже $1, и не выше $99999!");
  		if(playa != INVALID_PLAYER_ID)
  		{
    		if(ProxDetectorS(8.0, playerid, playa)&& IsPlayerInAnyVehicle(playa))
			{
   				if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
				{
    				if (GetVehicleModel(GetPlayerVehicleID(playerid)) == 525)
		     		{
					    if(playa == playerid) { SendClientMessage(playerid, COLOR_GREY, "Невозможно"); return 1; }
					    GetPlayerName(playa, giveplayer, sizeof(giveplayer));
						GetPlayerName(playerid, sendername, sizeof(sendername));
		    			format(string, sizeof(string), "* Вы предложили %s заправить его авто за $%d .",giveplayer,money);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						format(string, sizeof(string), "* Механик %s предлогает вам заправить ваше авто за $%d, (пиши /accept refill) чтобы согласиться.",sendername,money);
						SendClientMessage(playa, COLOR_LIGHTBLUE, string);
						RefillOffer[playa] = playerid;
						RefillPrice[playa] = money;
					}
					else SendClientMessage(playerid, COLOR_GREY, "	Вы не в машине механика.");
				}
			}
			else SendClientMessage(playerid, COLOR_GREY, "   Игрок не рядом с вами или не в машине.");
		}
	}
	else SendClientMessage(playerid, COLOR_GRAD2, "Заправить авто: /refill [id игрока] [цена]");
	return 1;
}
