stock LoadCar(playerid)
{
	new string[64];
	format(string, sizeof(string), "cars/%s.ini", GetName(playerid));
	if(fexist(string))
	{
	    new number[24],carid;
		new OpenCar = ini_openFile(string);
	 	ini_getInteger(OpenCar, "Model", CarInfo[playerid][cModel]);
	 	ini_getFloat(OpenCar, "Pos_X", CarInfo[playerid][cX]);
	 	ini_getFloat(OpenCar, "Pos_Y", CarInfo[playerid][cY]);
	 	ini_getFloat(OpenCar, "Pos_Z", CarInfo[playerid][cZ]);
	 	ini_getFloat(OpenCar, "Pos_F", CarInfo[playerid][cA]);
	 	ini_getInteger(OpenCar, "ColorOne", CarInfo[playerid][cColorOne]);
	 	ini_getInteger(OpenCar, "ColorTwo", CarInfo[playerid][cColorTwo]);
	 	ini_getString(OpenCar, "Number", number);
	 	ini_closeFile(OpenCar); // Закрываем файл.
	 	strmid(CarInfo[playerid][cNumer], number, 0, strlen(number), 255);
		carid = AddStaticVehicle(CarInfo[playerid][cModel], CarInfo[playerid][cX],CarInfo[playerid][cY],CarInfo[playerid][cZ],CarInfo[playerid][cA], CarInfo[playerid][cColorOne],CarInfo[playerid][cColorTwo]);
		SetPVarInt(playerid,"LichCarID",carid);
		GetVehicleParamsEx(GetPVarInt(playerid,"LichCarID"), engine, lights, alarm, doors, bonnet, boot, objective);
    	SetVehicleParamsEx(GetPVarInt(playerid,"LichCarID"), false, false, false, false, false, false, objective);
    	Cars[GetPVarInt(playerid,"LichCarID")] = 88;
    	Delete3DTextLabel(Text3D:CarInfoLabel[playerid]);
		format(string, 24, "%s {FF6000}[MG]",CarInfo[playerid][cNumer]);
		CarInfoLabel[playerid] = Create3DTextLabel( string, 0xAEFF00F6, 0.0, 0.0, 0.0, 20.0, 0, 1 );
		Attach3DTextLabelToVehicle(CarInfoLabel[playerid], GetPVarInt(playerid,"LichCarID"), 0.0, 0.0, 0.2);
		if(GetPVarInt(playerid,"LichCarID") == 65535)
		{
		    format(string, sizeof(string), "cars/%s.ini", GetName(playerid));
      		fremove(string);
		    SetPVarInt(playerid,"LichCarID",9999);
		}
	}
	return 1;
}

stock ini_GetKey( line[] )
{
	new keyRes[256];
	keyRes[0] = 0;
    if ( strfind( line , "=" , true ) == -1 ) return keyRes;
    strmid( keyRes , line , 0 , strfind( line , "=" , true ) , sizeof( keyRes) );
    return keyRes;
}

stock ini_GetValue( line[] )
{
	new valRes[256];
	valRes[0]=0;
	if ( strfind( line , "=" , true ) == -1 ) return valRes;
	strmid( valRes , line , strfind( line , "=" , true )+1 , strlen( line ) , sizeof( valRes ) );
	return valRes;
}

