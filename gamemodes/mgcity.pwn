stock ACResetPlayerWeapons(playerid)
{
    ResetPlayerWeapons(playerid);//отбираем пухи
	for(new i=0;i<47;i++) Weapons[playerid][i]=false;//обнуление
	for(new i=0;i<13;i++) GivePlayerWeapon(playerid,Weapons[playerid][i],0);//выдаём ему кулак
}

public OnPlayerUpdate(playerid)
{
    if(!IsPlayerNPC(playerid))
    {
		new weap = GetPlayerWeapon(playerid);
	    if(weap != 0 && !Weapons[playerid][weap] && weap != 40 && weap != 46 && weap != 2)//проверка на то что у нас не кулак, детонатор, парашут, а также читеское оружиее
    	{
			if(PlayerInfo[playerid][pAdmin] == 0 && gPlayerLogged[playerid] == 1)//если игрок с 0 адм и залогенен
			{
			    new Float:health;
    	    	GetPlayerHealth(playerid, health);
				if(health > 0.0)
				{
    				if (MPGO[playerid] == 0)
                    {
  						SendClientMessage(playerid, COLOR_LIGHTRED, "* Античит: Вы были кикнуты по подазрению в читерстве(код #9841).");
  						SendClientMessage(playerid, COLOR_LIGHTRED, "* Если вы считаете, что произошла ошибка, сообщите нам на наш форум.");
  						ACResetPlayerWeapons(playerid);//снимаем пухи с него
  					   	Kick(playerid);//кикаем
			     		return 1;
					}
				}
				return 1;
			}
		}
		if(PlayerInfo[playerid][pAdmin] <= 1999 && gPlayerLogged[playerid] == 1)//если игрок с 0 адм и залогенен
		{
			new Float:posx, Float:posy, Float:posz;//переменные
    		GetPlayerPos(playerid, posx, posy, posz);//проверка на координаты
		 	if(posz >= 2)
            {
				if(!IsPlayerInAnyVehicle(playerid))
				{
					if(PlayerInfo[playerid][pLevel] <= 1)
					{
					    if(!NotFlyingHack(playerid) && GetPlayerWeapon(playerid) != 46)
					    {
					        new animlib[30], animname[30];// CJ RUN
							GetAnimationName(GetPlayerAnimationIndex(playerid), animlib, sizeof(animlib), animname, sizeof(animname));
							if(strcmp(animlib, "SWIM", true) == 0 && strcmp(animname, "SWIM_crawl", true) == 0)
							{
								SendClientMessage(playerid, COLOR_LIGHTRED, "* Античит: Вы были кикнуты по подазрению в читерстве(код #1029).");
								SendClientMessage(playerid, COLOR_LIGHTRED, "* Если вы считаете, что произошла ошибка, сообщите нам на наш форум.");
								Kick(playerid);
							}
							return 1;
						}
						return 1;
					}
					return 1;
				}
				return 1;
			}
		}
		return 1;
	}
	return 1;
}

