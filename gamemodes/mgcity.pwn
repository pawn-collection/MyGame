CMD:tazer(playerid, params[])
{
	new sendername[MAX_PLAYER_NAME],giveplayer[MAX_PLAYER_NAME],string[256];
    if(pTazer[playerid] != 0)
   	{
		if(gTeam[playerid] == 2 || IsACop(playerid))
		{
	 		if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "   Нельзя использовать в машине !");
			new suspect = GetClosestPlayer(playerid);
		    if(IsPlayerConnected(suspect))
			{
	   			if(PlayerCuffed[suspect] > 0) return SendClientMessage(playerid, COLOR_GREY, "* Игрок в наручниках, либо тазер не перезарядился!");
			    if(GetDistanceBetweenPlayers(playerid,suspect) < 5)
				{
	    			if(PlayerInfo[playerid][pMember] != 7 || PlayerInfo[playerid][pLeader] != 7)
	   				{
				    	if(gTeam[suspect] == 2) return SendClientMessage(playerid, COLOR_GREY, "   Это твой коллега,одумайся!");
					    if(IsPlayerInAnyVehicle(suspect)) { SendClientMessage(playerid, COLOR_GREY, "   Подозреваемый в машине,вытащите его!"); return 1; }
					    GetPlayerName(suspect, giveplayer, sizeof(giveplayer));
						GetPlayerName(playerid, sendername, sizeof(sendername));
						format(string, sizeof(string), "* Вы были парализованны %s на 8 секунд.", sendername);
						SendClientMessage(suspect, COLOR_LIGHTBLUE, string);
						SetPlayerAttachedObject(suspect, 1, 18670, 1, -0.100000, -0.100000, -1.570000, -2.000000, 0.000000, 200.000000);
						PlayerPlaySound(playerid, 1057, 0.0, 0.0, 0.0);
						format(string, sizeof(string), "* Ты парализовал %s на 8 секунд.", giveplayer);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						pTazer[playerid] = pTazer[playerid] - 1;
						format(string, sizeof(string), "* %s выстрелил электрошоком в %s, и парализовал его(её).", sendername ,giveplayer);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
						SetPlayerAttachedObject(suspect, 1, 18670, 1, -0.100000, -0.100000, -1.580000, -2.000000, 0.000000, 200.000000);
						GameTextForPlayer(suspect, "~r~ELECTROSHOCK", 2500, 3);
						format(string, sizeof(string),"{FF0000}!**  Вы были парализованы Электрошоком  **!");
	    				ShowDialog(suspect, 9161, 0, "********** ЭЛЕКТРОШОК **********", string, "OK", "OK");
	       				SetPlayerChatBubble(suspect,"А-а-а-а-а-а!!!",COLOR_RED,45.0,10000);
						TogglePlayerControllable(suspect, 0);
						ApplyAnimation(suspect,"SWEET","LaFin_Sweet",4.1,0,1,1,1,1);
						ApplyAnimation(suspect,"SWEET","LaFin_Sweet",4.1,0,1,1,1,1);
						PlayerCuffed[suspect] = 1; PlayerCuffedTime[suspect] = 8;
					}
				}
				else SendClientMessage(playerid, COLOR_GREY, "Никого нет рядом!");
			}
		}
		else SendClientMessage(playerid, COLOR_GREY, "Вы не законник!");
	}
	else
	{
		SendClientMessage(playerid, COLOR_GREY, "  Аккумулятор ЭлектроШокера разряжен! (/tazerinfo)");
		SendClientMessage(playerid, COLOR_GREY, "  Перезарядка на базе - {FF9000}/duty");
	}
	return 1;
}
