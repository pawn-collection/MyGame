						ShowDialog(para1,116,DIALOG_STYLE_MSGBOX,"Приглашение",string,"Да","Нет");
				   	}
					else SendClientMessage(playerid, COLOR_GREY, "  Этот игрок в розыске / Меняет команду / Или не готов к поступлению.");
