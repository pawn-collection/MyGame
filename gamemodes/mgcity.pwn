public CheckGas()
{
	new string[256];
	foreach (Player, i)
	{
    	new vehicle = GetPlayerVehicleID(i);
		if(GetPlayerState(i) == PLAYER_STATE_DRIVER && !IsAVello(GetPlayerVehicleID(i)) && !IsAPlane(GetPlayerVehicleID(i)) && !IsABoat(GetPlayerVehicleID(i)))
 		{
  			if(Gas[vehicle] >= 1)
		   	{
		   	    if(VehInfo[vehicle][vEngine] == 1)
				{
         			if(gGas[i] == 1) {
	      			format(string, sizeof(string), "  ~n~ ~n~~p~Benzin: ~y~%d%", Gas[vehicle]);
				   	TextDrawShowForPlayer(i, benzin[i]); TextDrawSetString(benzin[i], string); }
					if(IsAPlane(vehicle) || IsABoat(vehicle)) { Gas[vehicle]++; { gGas[i] = 0; } }
	   				Gas[vehicle]--;
  				}
 			}
  			else
   			{
         		TogglePlayerControllable(i, 0);
	            Stopeee[i] = 1;
	            Stopee[i] = 3;
            	GetVehicleParamsEx(vehicle,engine,lights,alarm,doors,bonnet,boot,objective);
           		SetVehicleParamsEx(vehicle,false,lights,alarm,doors,bonnet,boot,objective);
           		SendClientMessage(i, COLOR_WHITE, "* {FFFFFF}В транспорте нет бензина, двигатель заглох. {FF9000}[Y] {FFFFFF}чтобы завести *");
           		format(string, sizeof(string), "~r~NO FUEL");
    			GameTextForPlayer(i, string, 3000, 3);
    			VehInfo[vehicle][vEngine] = 0;
			}
    	}
	    new Float:health;
	    GetPlayerHealth(i, health);
		SetPlayerHealth(i, health - 1.0);
		switch(STDPlayer[i])
		{
			case 1:
			{
			    GetPlayerHealth(i, health);
			    SetPlayerHealth(i, health - 5.0);
			    SendClientMessage(i, COLOR_LIGHTBLUE, "* Потерянный 4 здоровья из-за STD.");
			}
			case 2:
			{
			    GetPlayerHealth(i, health);
			    SetPlayerHealth(i, health - 12.0);
			    SendClientMessage(i, COLOR_LIGHTBLUE, "* Потерянный 8 здоровья из-за STD.");
			}
			case 3:
			{
			    GetPlayerHealth(i, health);
			    SetPlayerHealth(i, health - 20.0);
			    SendClientMessage(i, COLOR_LIGHTBLUE, "* Потерянный 12 здоровья из-за STD.");
			}
		}
		if(oGetPlayerMoney(i) < 0)
		{
		    if(GetPVarInt(i,"MoneyMessage")==0)
		    {
			    format(string, sizeof(string), "* У вас долг, вы должны отдать сумму: $%d или сядете в тюрьму.", oGetPlayerMoney(i));
				SendClientMessage(i, COLOR_LIGHTRED, string);
				format(string, sizeof(string), "* Есть масса способов заработать,подробнее - {FF9000}/job");
				SendClientMessage(i, COLOR_LIGHTRED, string);
				SetPVarInt(i,"MoneyMessage",1);
			}
		}
		else SetPVarInt(i,"MoneyMessage",0);
	}
	return 1;
}


public Fillup()
{
	foreach(Player,i)
	{
    	new VID,FillUp,string[256];
		VID = GetPlayerVehicleID(i);
		FillUp = GasMax - Gas[VID];
		if(Refueling[i] == 1)
	 	{
			if(PlayerInfo[i][pMoney] >= FillUp+4)
			{
				Gas[VID] += FillUp;
				FillUp = FillUp * SBizzInfo[3][sbEntranceCost];
				format(string,sizeof(string),"* Ваша машина заправлена 95-ым бензином, за: $%d.",FillUp);
				SendClientMessage(i,COLOR_LIGHTBLUE,string);
				oGivePlayerMoney(i, - FillUp);
				SBizzInfo[3][sbTill] += FillUp;
				ExtortionSBiz(3, FillUp);
				Refueling[i] = 0;
			}
			else
			{
			   	format(string,sizeof(string),"* Вам не хватает денег для заправки. Требуется: $%d",FillUp);
				SendClientMessage(i,COLOR_LIGHTBLUE,string);
		 	}
		}
	}
	return 1;
}

