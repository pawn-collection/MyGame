CMD:lock(playerid, params[])
{
	new Float: X, Float: Y, Float: Z;
    if(GetPVarInt(playerid, "RENTCAR") != 9999)
    {
        GetVehiclePos(GetPVarInt(playerid, "RENTCAR"), X, Y, Z);
        if(IsPlayerInRangeOfPoint(playerid, 8.0, X, Y, Z))
        {
        	if(LOCKEDVEH[GetPVarInt(playerid, "RENTCAR")] == 0) { LockCar(GetPVarInt(playerid, "RENTCAR")); GameTextForPlayer(playerid, "~w~CЏOPOмм•• –AЋ…мA ~r~€AKP‘ЏA", 5000, 6); }
        	else if(LOCKEDVEH[GetPVarInt(playerid, "RENTCAR")] == 1) { UnLockCar(GetPVarInt(playerid, "RENTCAR")); GameTextForPlayer(playerid, "~w~CЏOPOмм•• –AЋ…мA ~g~OЏKP‘ЏA", 5000, 6); }
            PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Вы должны быть около машины.");
	}
    new house = PlayerInfo[playerid][pPhousekey];
    if(house != 9999 )
	{
 		GetVehiclePos(HouseVehicle[house], X, Y, Z);
   		if(IsPlayerInRangeOfPoint(playerid, 8.0, X, Y, Z))
		{
		   	if(LOCKEDVEH[HouseVehicle[house]] == 0) { LockCar(HouseVehicle[house]); GameTextForPlayer(playerid, "~w~ѓO–AЋм•• –AЋ…мA ~r~€AKP‘ЏA", 5000, 6); }
     		else if(LOCKEDVEH[HouseVehicle[house]] == 1) { UnLockCar(HouseVehicle[house]); GameTextForPlayer(playerid, "~w~ѓO–AЋм•• –AЋ…мA ~g~OЏKP‘ЏA", 5000, 6); }
           	PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Вы должны быть около машины.");
	}
	return 1;
}
