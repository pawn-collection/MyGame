CMD:cvet(playerid, params[])
{
    new tmp[5],string[256];
    if (!sscanf(params, "s[5]", tmp))
    {
        if (strval(tmp)>100 || strval(tmp)<0) return SendClientMessage(playerid, COLOR_WHITE, "{FFFFFF}Используйте: {FF9000}/cvet [0-100]");
        else
        {
            SetPlayerColor(playerid,playerColors[strval(tmp)]);
            format(string, sizeof(string), "{FF9000}Вы изменили свой цвет на {FFFFFF}%d.", strval(tmp));
			SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
        }
   	}
   	else SendClientMessage(playerid, COLOR_WHITE, "{FFFFFF}Используйте: {FF9000}/cvet [0-100]");
   	return 1;
}
