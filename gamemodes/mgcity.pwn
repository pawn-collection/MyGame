    new playa,tmp[24],money,result[64];
	if (!sscanf(params, "s[24]is[64]", tmp, money,result))
	{
	    playa = ReturnUser(tmp);
 		if(PlayerInfo[playerid][pMember] == 7 || PlayerInfo[playerid][pLeader] == 7) { return 1; }
		new turnmes[128];
		if(IsACop(playerid))
		{
			if(IsPlayerConnected(playa))
			{
				if(playa != INVALID_PLAYER_ID)
				{
					if(WantedLevel[playa] > 10 || WantedPoints[playa] > 10) return SendClientMessage(playerid, COLOR_GRAD1, "У этого игрка уже 10 звёзд");
					if(money < 0 || money > 10) return SendClientMessage(playerid, COLOR_GRAD1, "Введённое число должно быть больше 1, но не меньше 10.");
					if(IsACop(playa) || PlayerInfo[playa][pPriziv] >= 1) return SendClientMessage(playerid, COLOR_GRAD1, "Нельзя дать розыск Представителю Закона/Призывнику.");
					if(PlayerInfo[playa][pJailed] == 0)
					{
						foreach (Player, i)
	       				{
	       				    if(IsACop(i))
	       				    {
	       						format(cbjstore, sizeof(cbjstore), "[%s] по рации: Совершено преступление: [%s],Подозреваемый: [%s],Ур. розыска: [%d]",GetName(playerid),(result),GetName(playa),money);
	       						SendClientMessage(i, COLOR_LIGHTNEUTRALBLUE, cbjstore);
	        				}
	       				}
	       				SetPlayerCriminal(playa,playerid, (result));
	       				format(turnmes, sizeof(turnmes), " Вы совершили Преступление [%s]. Стукач: [%s]. {FF0000}Ур. розыска: [%d]",(result),GetName(playerid),money);
	                   	SendClientMessage(playa, COLOR_YELLOW, turnmes);
	                   	WantedPoints[playa] += money;
						WantedLevel[playa] += money;
     				}
				}
			}
		}
		else SendClientMessage(playerid, COLOR_GRAD1, " Вы не можете использовать эту команду!");
	}
	else SendClientMessage(playerid, COLOR_GRAD2, "*Используйте: /su [id игрока/часть ника] [кол-во звёзд] [причина]");
	return 1;
}
