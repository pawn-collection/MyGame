stock RemovePlayerWeapon(playerid, weaponid)
{
	new plyWeapons[13] = 0,plyAmmo[13] = 0;
	for(new slot = 0; slot != 13; slot++)
	{
		new wep, ammo;
		GetPlayerWeaponData(playerid, slot, wep, ammo);
		if(wep == weaponid) ACGivePlayerWeapon(playerid,weaponid,-ammo);
		if(wep != weaponid && ammo != 0) GetPlayerWeaponData(playerid, slot, plyWeapons[slot], plyAmmo[slot]);
	}
	ACResetPlayerWeapons(playerid);
	for(new slot = 0; slot != 13; slot++){
	    if(plyAmmo[slot] != 0){
			ACGivePlayerWeapon(playerid, plyWeapons[slot], plyAmmo[slot]);
		}
	}
	return 1;
}

