CMD:mdc(playerid, params[])
{
    new giveplayerid,string[256],sendername[MAX_PLAYER_NAME];
	if (!sscanf(params, "i", giveplayerid))
	{
	    if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_GREY, "  Вы не полицейский !");
		new tmpcar = GetPlayerVehicleID(playerid);
		if(IsACopCar(tmpcar)||IsPlayerInRangeOfPoint(playerid,5.0, 253.9280,69.6094,1003.6406))
		{
			if(IsPlayerConnected(giveplayerid))
			{
			    if(giveplayerid != INVALID_PLAYER_ID)
			    {
			        GetPlayerName(giveplayerid, sendername, sizeof(sendername));
					SendClientMessage(playerid, TEAM_BLUE_COLOR,"{FF9000}______** {FFFFFF}База Данных Гос.Служб {FF9000}**_______");
					format(string, sizeof(string), "*Имя : {FF9000}%s", sendername);
					SendClientMessage(playerid, COLOR_WHITE,string);
					format(string, sizeof(string), "*Преступление : {FF9000}%s", PlayerCrime[giveplayerid][pAccusedof]);
					SendClientMessage(playerid, COLOR_GRAD2,string);
					format(string, sizeof(string), "*Претендент : {FF9000}%s", PlayerCrime[giveplayerid][pVictim]);
					SendClientMessage(playerid, COLOR_GRAD3,string);
					format(string, sizeof(string), "*Сообщение : {FF9000}%s", PlayerCrime[giveplayerid][pAccusing]);
					SendClientMessage(playerid, COLOR_GRAD4,string);
					format(string, sizeof(string), "*Обвиняемый : {FF9000}%s", PlayerCrime[giveplayerid][pBplayer]);
					SendClientMessage(playerid, COLOR_GRAD5,string);
					SendClientMessage(playerid, TEAM_BLUE_COLOR,"{FF9000}_______________________________________");
				}
			}
			else SendClientMessage(playerid, COLOR_GREY, "   Этот игрок вышел !");
		}
		else SendClientMessage(playerid, COLOR_GRAD2, "   Вы не в полицейской машине или Департаменте полиции.");
	}
	else SendClientMessage(playerid, COLOR_GRAD2, "*Используйте: /mdc [id игрока/часть ника]");
	return 1;
}
//----------------------------------[SetCrim]-----------------------------------------------
CMD:tsu(playerid, params[]) return cmd_tsuspect(playerid, params);
CMD:tsuspect(playerid, params[])
{
    new giveplayerid,result[64],sendername[MAX_PLAYER_NAME],giveplayer[MAX_PLAYER_NAME];
	if (!sscanf(params, "is[64]", giveplayerid,result))
	{
	    if(OnDuty[playerid] != 1  && PlayerInfo[playerid][pMember] == 1) return SendClientMessage(playerid, COLOR_GREY, "   Вы не на службе (/duty)!");
        if(WantedLevel[giveplayerid] > 10 || WantedPoints[giveplayerid] > 10) return SendClientMessage(playerid, COLOR_GRAD1, "У этого игрка уже 10 звёзд");
		if(IsACop(giveplayerid) || PlayerInfo[giveplayerid][pPriziv] >= 1) return SendClientMessage(playerid, COLOR_GRAD1, "Нельзя дать розыск Представителю Закона/Призывнику.");
		if(PlayerInfo[giveplayerid][pJailed] != 0) return SendClientMessage(playerid, COLOR_GRAD1, "Этот игрок находитсья в тюрьме");
		if(PlayerInfo[playerid][pMember] == 7 || PlayerInfo[playerid][pLeader] == 7) { }
		if(IsACop(playerid))
		{
			if(IsPlayerConnected(giveplayerid))
			{
			    if(giveplayerid != INVALID_PLAYER_ID)
			    {
					if (!IsACop(giveplayerid))
					{
						GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
						GetPlayerName(playerid, sendername, sizeof(sendername));
						if(PlayerInfo[giveplayerid][pJailed] == 0)
						{
							if(WantedPoints[giveplayerid] == 0) { WantedPoints[giveplayerid] = 3; }
							else { WantedPoints[giveplayerid]+= 2; }
							SetPlayerCriminal(giveplayerid,playerid, result);
							return 1;
						}
						else SendClientMessage(playerid, COLOR_GRAD2, "* Вы не можете обвинить заключённого!");
					}
					else SendClientMessage(playerid, COLOR_GRAD2, "   Вы не можете обвинить полицейского !");
				}
			}
			else SendClientMessage(playerid, COLOR_GRAD1, "* Не активный игрок.");
		}
		else SendClientMessage(playerid, COLOR_GRAD2, "   Вы не полицейский / агент ФБР / Солдат национальной гвардии !");
	}
	else SendClientMessage(playerid, COLOR_GRAD2, "*Используйте: (/su)spect [id игрока/часть ника] [состав преступления]");
	return 1;
}
