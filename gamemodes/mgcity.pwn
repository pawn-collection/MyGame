public CarTow(carid)
{
	foreach (Player, i)
	{
		if(IsPlayerInVehicle(i, carid))
		{
			return 0;
		}
	}
	SetVehiclePos(carid,HouseCarSpawns[carid-1][0], HouseCarSpawns[carid-1][1], HouseCarSpawns[carid-1][2]);
	SetVehicleZAngle(carid, HouseCarSpawns[carid-1][3]);
	return 1;
}

stock CarRespawn(carid)
{
	foreach (Player, i)
	{
		if(IsPlayerInVehicle(i, carid)) return 0;
	}
	SetVehicleToRespawn(carid);
	return 1;
}

public LockCar(carid)
{
    LOCKEDVEH[carid] = 1;
	foreach (Player, i) SetVehicleParamsForPlayer(carid,i,0,1);
	return 1;
}

public UnLockCar(carid)
{
    LOCKEDVEH[carid] = 0;
	foreach (Player, i) SetVehicleParamsForPlayer(carid,i,0,0);
	return 1;
}

public OnPlayerExitVehicle(playerid, vehicleid)
{
    printf("OnPlayerExitVehicle %s");
    if (GetPlayerState(playerid) == 1)
	{
	    TextDrawHideForPlayer(playerid, benzin[playerid]);
	    SetCameraBehindPlayer(playerid);
		return 1;
	}
	if (GetPlayerState(playerid) == 1)
	{
		return 1;
	}
	if(gGas[playerid] == 1)
	{
	    GameTextForPlayer(playerid, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~Left car", 500, 3);
	    TextDrawHideForPlayer(playerid, benzin[playerid]);
	}
	foreach(Player,x)
	{
	    if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && gSpectateID[x] == playerid && gSpectateType[x] == ADMIN_SPEC_TYPE_VEHICLE)
		{
	        TogglePlayerSpectating(x, 1);
	        PlayerSpectatePlayer(x, playerid);
	        gSpectateType[x] = ADMIN_SPEC_TYPE_PLAYER;
		}
	}
	return 1;
}

