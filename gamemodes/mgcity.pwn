CMD:pay(playerid, params[])
{
	new giveplayerid,moneys;
    if (!sscanf(params, "ii", giveplayerid, moneys))
 	{
		if(moneys > 1000 && PlayerInfo[playerid][pLevel] < 3) return SendClientMessage(playerid, COLOR_GRAD1, " Вы должны быть больше 3 уровня чтобы передавать больше 1000!");
		if(PlayerInfo[playerid][pConnectTime] == 0) return SendClientMessage(playerid, COLOR_GRAD2, " Вы не можете передавать деньги, т.к. отыграли на сервере меньше 1 часа!");
		if(moneys < 1 || moneys > 99999) return SendClientMessage(playerid, COLOR_GRAD1, "Введённое число должно быть больше 1, но не меньше 99999.");
		new playerip[256];
		GetPlayerIp(playerid, playerip, sizeof(playerip));
		new giveplayerip[256];
		GetPlayerIp(giveplayerid, giveplayerip, sizeof(giveplayerip));
		if(!strcmp(playerip,giveplayerip,true)) return SendClientMessage(playerid, COLOR_GRAD1, "У вас одинаковые IP!");
		if (IsPlayerConnected(giveplayerid))
		{
		    if(giveplayerid != INVALID_PLAYER_ID)
		    {
				if (ProxDetectorS(5.0, playerid, giveplayerid))
				{
				    new sendername[MAX_PLAYER_NAME],giveplayer[MAX_PLAYER_NAME],string[256];
					GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
					GetPlayerName(playerid, sendername, sizeof(sendername));
					new playermoney = PlayerInfo[playerid][pMoney];
					if (moneys > 0 && playermoney >= moneys)
					{
					    if(giveplayerid == playerid) { SendClientMessage(playerid, COLOR_GREY, "Вы не можете передавать деньги самому себе!"); return 1; }
						oGivePlayerMoney(playerid, (0 - moneys));
						oGivePlayerMoney(giveplayerid, moneys);
						format(string, sizeof(string), "   Вы передали %s(%d), {00BC12}$%d.", giveplayer,giveplayerid, moneys);
						PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
						SendClientMessage(playerid, COLOR_GRAD1, string);
						format(string, sizeof(string), "   Вы получили $%d от %s(%d).", moneys, sendername, playerid);
						SendClientMessage(giveplayerid, COLOR_GRAD1, string);
						format(string, sizeof(string), "%s получил $%d to %s", sendername, moneys, giveplayer);
						PayLog(string);
						if(moneys >= 1000000) ABroadCast(COLOR_YELLOW,string,1);
						PlayerPlaySound(giveplayerid, 1052, 0.0, 0.0, 0.0);
						format(string, sizeof(string), "* %s передаёт немного наличных %s.", sendername ,giveplayer);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					}
					else SendClientMessage(playerid, COLOR_GRAD1, "   У вас нет столько денег.");
				}
				else SendClientMessage(playerid, COLOR_GRAD1, "   Вы слишком далеко.");
			}
		}
		else SendClientMessage(playerid, COLOR_GRAD1, "   Этого игрока нету на сервере");
  	}
  	else SendClientMessage(playerid, COLOR_GRAD2, "* Подсказка: /pay [id игрока] [сумма]");
	return 1;
}
