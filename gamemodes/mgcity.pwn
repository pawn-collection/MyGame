CMD:carcolor(playerid, params[])
{
    new colornum,color,playername[MAX_PLAYER_NAME],string[256];
	if (!sscanf(params, "ii", colornum,color))
	{
		new house = PlayerInfo[playerid][pPhousekey];
		GetPlayerName(playerid, playername, sizeof(playername));
		if (house != 9999 && strcmp(playername, HouseInfo[PlayerInfo[playerid][pPhousekey]][hOwner], true) == 0)
		{
			if(PlayerInfo[playerid][pDonateRank] < 2)
	    	{
				if(color < 0 || color > 126) return SendClientMessage(playerid, COLOR_GRAD3," Максимум  126");
			}
			if(PlayerInfo[playerid][pDonateRank] >= 2)
	    	{
				if(color < 0 || color > 160) return SendClientMessage(playerid, COLOR_GRAD3," Максимум  160");
			}
			if (oGetPlayerMoney(playerid) < 5000) return SendClientMessage(playerid, COLOR_GRAD3," Вы не можете позволить себе это ($5000)");
			if(colornum == 1)
			{
				HouseInfo[house][hVcol1] = color;
				format(string, sizeof(string), " Первый цвет установлен %d -$5000",color);
				SendClientMessage(playerid, COLOR_GRAD3,string);
			}
			else
			{
				HouseInfo[house][hVcol2] = color;
				format(string, sizeof(string), " Второй цвет установлен %d -$5000",color);
				SendClientMessage(playerid, COLOR_GRAD3,string);
			}
			SendClientMessage(playerid, COLOR_GRAD6, " Покрасочные работы будут завершены через 24 часа");
			oGivePlayerMoney(playerid,-5000);
			SBizzInfo[5][sbTill] += 5000;
			ExtortionSBiz(5, 5000);
			PlayerPlaySound(playerid, 1134, 0.0, 0.0, 0.0);
		}
		else return SendClientMessage(playerid, COLOR_GRAD2, "   Вы не владеете домом");
	}
	else SendClientMessage(playerid, COLOR_GRAD3,"($5000) ИСПОЛЬЗУЙТЕ: /carcolor [color 1 - 2] [color number]");
	return 1;
}
