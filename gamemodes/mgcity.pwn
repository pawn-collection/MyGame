								format(string, sizeof(string), "<< Полицейский SFPD %s аррестовал преступника %s >>", sendername, giveplayer);
								OOCNews(COLOR_LIGHTRED, string);
							}
							else if(PlayerInfo[playerid][pMember]==2||PlayerInfo[playerid][pLeader]==2)
							{
								format(string, sizeof(string), "<< Агент FBI %s аррестовал преступника %s >>", sendername, giveplayer);
								OOCNews(COLOR_LIGHTRED, string);
							}
							else if(PlayerInfo[playerid][pMember]==21||PlayerInfo[playerid][pLeader]==21)
							{
								format(string, sizeof(string), "<< Боец SWAT %s аррестовал преступника %s >>", sendername, giveplayer);
								OOCNews(COLOR_LIGHTRED, string);
							}
							else if(PlayerInfo[playerid][pMember]==3||PlayerInfo[playerid][pLeader]==3)
							{
								format(string, sizeof(string), "<< Военный %s аррестовал преступника %s >>", sendername, giveplayer);
								OOCNews(COLOR_LIGHTRED, string);
							}
							PlayerInfo[suspect][pJailed] = 1;
							SetPlayerInterior(suspect, 6);
							PlayerInfo[suspect][pJailTime] = time * 60;
							if(bail == 1)
							{
								JailPrice[suspect] = bailprice;
								format(string, sizeof(string), "* Вас посадили в тюрьму на %d Секунд.   Залог: $%d", PlayerInfo[suspect][pJailTime], JailPrice[suspect]);
								SendClientMessage(suspect, COLOR_LIGHTBLUE, string);
							}
							else
							{
							    JailPrice[suspect] = 0;
								format(string, sizeof(string), "* Вас посадили в тюрьму на %d Секунд.   Залог: возможен", PlayerInfo[suspect][pJailTime]);
								SendClientMessage(suspect, COLOR_LIGHTBLUE, string);
							}
							oGivePlayerMoney(playerid, 5000);
							PlayerInfo[suspect][pJailed] = 1;
					        PlayerInfo[suspect][pArrested] += 1;
							SetPlayerFree(suspect,playerid, "* Арестованы!");
							WantedPoints[suspect] = 0;
							WantedLevel[suspect] = 0;
							SetPVarInt(suspect,"WantLawyer",0);
							SpawnPlayer(suspect);
						}
						else return SendClientMessage(playerid, COLOR_GRAD2, "Игрок в тюрьме");
					}
					else return SendClientMessage(playerid, COLOR_GREY, "Вы не около камер");
				}
			}
			else return SendClientMessage(playerid, COLOR_GREY, "Рядом с вами никого нет");
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Вы не законник!");
	}
	else SendClientMessage(playerid, COLOR_GRAD2, "{FFFFFF}Используйте: {FF8000} /arrest [время] [залог (0=нету 1=есть)] [стоимость залога (0=нету)]");
	return 1;
}
CMD:contract(playerid,params[])
{
	new giveplayerid,moneys,giveplayer[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME],string[256];
 	if (!sscanf(params, "ii",giveplayerid,moneys))
	{
		if(moneys < 3000 || moneys > 1000000) return SendClientMessage(playerid, COLOR_GREY, "   Вы можете поставить контракт не меньше чем $3000, и не больше чем $1000000!");
		if(PlayerInfo[playerid][pLevel] < 0) return SendClientMessage(playerid, COLOR_GRAD1, "* Вы должны быть 1 уровня чтобы заключать контракт");
		if(giveplayerid == INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_GRAD1, "Игрок не подключен");
		if(PlayerInfo[playerid][pMoney] <= moneys) return SendClientMessage(playerid, COLOR_GRAD1, "У вас не хватает денег.");
 		if(PlayerInfo[giveplayerid][pMember] == 8 && PlayerInfo[playerid][pMember] == 8) return SendClientMessage(playerid, COLOR_GREY, "   Вы неможете установить Контракты в вашем собственном Агентстве!");
  		if(PlayerInfo[giveplayerid][pMember] == 8 || PlayerInfo[giveplayerid][pLeader] == 8) return SendClientMessage(playerid, COLOR_GREY, "   Нельзя дать заказ на Хитмана !");
    	if(PlayerInfo[playerid][pMember] == 8 || PlayerInfo[playerid][pLeader] == 8) return SendClientMessage(playerid, COLOR_GREY, "	Вы не можете давать заказ!");
	    if(gTeam[giveplayerid] == 2 && moneys != 3000) return SendClientMessage(playerid, COLOR_GREY, "   Только $3000 может быть установлено за контракт на полицейского,нам самим приятно выполнять =)");
	    if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_GREY, "*Вы неможете поставить на себя!");
		GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
		GetPlayerName(playerid, sendername, sizeof(sendername));
		new playermoney = oGetPlayerMoney(playerid);
		if (moneys < 0 && playermoney < moneys) return SendClientMessage(playerid, COLOR_GRAD1, "Неправильное значение.");
		oGivePlayerMoney(playerid, (0 - moneys));
		PlayerInfo[giveplayerid][pHeadValue]+=moneys;
		format(string, sizeof(string), "%s сделал заказ на убийство %s, за $%d.",sendername, giveplayer, moneys);
		SendFamilyMessage(8, COLOR_YELLOW, string);
		format(string, sizeof(string), "Вы заключили контракт на убийство %s, за $%d.",giveplayer, moneys);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
		return 1;
	}
	else SendClientMessage(playerid, COLOR_GRAD1, "Используйте: /contract [IDигрока/Ник Игрока] [сумма]");
	return 1;
}
