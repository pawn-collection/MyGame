{
    new plo;
    if (!sscanf(params, "i", plo))
	{
		new Float:plocx,Float:plocy,Float:plocz;
		if (IsPlayerConnected(plo))
		{
		    if(plo != INVALID_PLAYER_ID)
	    	{
				if (PlayerInfo[playerid][pAdmin] >= 2)
				{
					if(Spectate[playerid] != 555)
					{
						Spectate[playerid] = 556;
					}
					GetPlayerPos(plo, plocx, plocy, plocz);
					if(PlayerInfo[plo][pInt] > 0)
					{
						SetPlayerInterior(playerid,PlayerInfo[plo][pInt]);
						SetPlayerInterior(playerid,GetPlayerInterior(plo));
						SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(plo));
						PlayerInfo[playerid][pInt] = PlayerInfo[plo][pInt];
						PlayerInfo[playerid][pLocal] = PlayerInfo[plo][pLocal];
					}
					if(PlayerInfo[playerid][pInt] == 0)
					{
						SetPlayerInterior(playerid,0);
					}
					if(plocz > 530.0 && PlayerInfo[plo][pInt] == 0) //the highest land point in sa = 526.8
					{
						SetPlayerInterior(playerid,1);
						PlayerInfo[playerid][pInt] = 1;
					}
					if (GetPlayerState(playerid) == 2)
					{
						new tmpcar = GetPlayerVehicleID(playerid);
						SetPlayerInterior(playerid,GetPlayerInterior(plo));
						SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(plo));
						SetVehiclePos(tmpcar, plocx, plocy+4, plocz);
						TelePos[playerid][0] = 0.0;TelePos[playerid][1] = 0.0;
					}
					else
					{
						SetPlayerPos(playerid,plocx,plocy+2, plocz);
					}
					SendClientMessage(playerid, COLOR_GRAD1, "   Вы были телепортированы");
				}
				else SendClientMessage(playerid, COLOR_GRAD1, "   Вы не можете использовать эту команду!");
			}
		}
		else SendClientMessage(playerid, COLOR_GRAD1, "Этот игрок оффлайн");
	}
	else cmd_tp(playerid,params);
	return 1;
