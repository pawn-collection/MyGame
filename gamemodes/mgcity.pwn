CMD:duty(playerid, params[])
{
    new string[256],sendername[MAX_PLAYER_NAME];
	GetPlayerName(playerid, sendername, sizeof(sendername));
	if(PlayerInfo[playerid][pMember] == 1 || PlayerInfo[playerid][pLeader] == 1 || PlayerInfo[playerid][pLeader] == 2 || PlayerInfo[playerid][pMember] == 2 || PlayerInfo[playerid][pLeader] == 3 || PlayerInfo[playerid][pMember] == 3 || PlayerInfo[playerid][pLeader] == 21 || PlayerInfo[playerid][pMember] == 21 || PlayerInfo[playerid][pLeader] == 22 || PlayerInfo[playerid][pMember] == 22)  //
	{
		if (IsPlayerInRangeOfPoint(playerid,3.0,2276.4055,2424.9768,3.4766) || IsPlayerInRangeOfPoint(playerid,3.0,-2241.2517,2357.9685,4.9796) || IsPlayerInRangeOfPoint(playerid,3.0,201.0913,1869.3828,13.1406) || IsPlayerInRangeOfPoint(playerid,3.0,326.6906,308.9922,999.1484) || IsPlayerInRangeOfPoint(playerid,3.0,1568.625122,-1690.535766,5.890600)
	   	|| IsPlayerInRangeOfPoint(playerid,3.0,-1594.1844,716.1373,-4.9063) || IsPlayerInRangeOfPoint(playerid,3.0,1803.3815,-1531.7141,5700.4302))
		{
			if(OnDuty[playerid]==0)
	        {
			   	format(string, sizeof(string), "* %s берёт значёк и оружие из шкафчика", sendername);
				ProxDetector(20.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
				ACGivePlayerWeapon(playerid, 24, 70);
				ACGivePlayerWeapon(playerid, 3, 1);
				SetPlayerAttachedObject(playerid, 1, 18637, 14, 0.0, 0.0, 0.0, 0.0, 180.0, 180.0);
				SetPlayerHealth(playerid, 100);
                SetArm(playerid,100);
                pTazer[playerid] = 10;
                SetPVarInt(playerid,"SHIELDD",1);
				OnDuty[playerid] = 1;
			}
			else if(OnDuty[playerid]==1)
			{
				format(string, sizeof(string), "* %s кладёт значёк и оружие в шкафчик", sendername);
				ProxDetector(20.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
				RemovePlayerAttachedObject(playerid,1);
				ACResetPlayerWeapons(playerid);
				SetPVarInt(playerid,"SHIELDD",0);
				pTazer[playerid] = 0;
				OnDuty[playerid] = 0;
			}
		}
		else SendClientMessage(playerid, COLOR_GRAD2, "  Тут невозможно использовать /duty!");
	}
	else if(PlayerInfo[playerid][pMember] == 4||PlayerInfo[playerid][pLeader] == 4)
	{
 		if (PlayerInfo[playerid][pRank] == 8 || PlayerInfo[playerid][pRank] == 9)
   		{
     		if(JobDuty1[playerid] == 1) { SendClientMessage(playerid, COLOR_LIGHTBLUE, "* [М.Ч.С.] Вы закончили дежурство [OFF]"); JobDuty1[playerid] = 0;}
	    	else { SendClientMessage(playerid, COLOR_LIGHTBLUE, "* [М.Ч.С.] Вы заступили на дежурство [ON]"); JobDuty1[playerid] = 1;}
		}
		else
		{
	  		if(JobDuty[playerid] == 1) { SendClientMessage(playerid, COLOR_LIGHTBLUE, "* [Минздрав] Вы закончили дежурство"); JobDuty[playerid] = 0; Medics -= 1;}
	  		else { SendClientMessage(playerid, COLOR_LIGHTBLUE, "* [Минздрав] Вы заступили на дежурство"); JobDuty[playerid] = 1; Medics += 1;}
		}
	}
	else if(PlayerInfo[playerid][pJob] == 7)
	{
		if(JobDuty[playerid] == 1) { SendClientMessage(playerid, COLOR_LIGHTBLUE, "* [Механник] Вы заступили на дежурство"); JobDuty[playerid] = 0; Mechanics -= 1; }
	  	else { SendClientMessage(playerid, COLOR_LIGHTBLUE, "* [Механник] Вы закончили дежурство"); JobDuty[playerid] = 1; Mechanics += 1; }
	}
	return 1;
}

