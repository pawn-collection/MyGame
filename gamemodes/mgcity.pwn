public SearchingHit(playerid)
{
	new string[256];
	new searchhit = 0;
	foreach (Player, i)
	{
		if(IsPlayerConnected(i))
		{
		    if(searchhit == 0)
		    {
			    if(PlayerInfo[i][pHeadValue] > 0 && GotHit[i] == 0 && PlayerInfo[i][pMember] != 8)
			    {
			        searchhit = 1;
			        hitfound = 1;
			        hitid = i;
			        foreach(Player,k)
					{
						if(IsPlayerConnected(k))
						{
				        	if(PlayerInfo[k][pMember] == 8 || PlayerInfo[k][pLeader] == 8)
				        	{
	               				SendClientMessage(k, COLOR_WHITE, "|__________________ {FF0000}Hitmans Agency News {FFFFFF}__________________|");
				                SendClientMessage(k, COLOR_DBLUE, "*** Новвое сообщение, доступен новый контракт. ***");
				                format(string, sizeof(string), "*Жертва: %s   ID: %d   Вознаграждение: $%d", GetName(i), i, PlayerInfo[i][pHeadValue]);
								SendClientMessage(k, COLOR_DBLUE, string);
								SendClientMessage(k, COLOR_YELLOW, "*Используйте giveHIT чтобы получить контракт на убийство.");
								SendClientMessage(k, COLOR_WHITE, "|________________________________________________________|");
	      					}
					    }
					}
					return 0;
			    }
			}
		}
	}
	if(searchhit == 0) SendClientMessage(playerid, COLOR_GREY, "   Нет доступных контрактов !");
	return 0;
}

