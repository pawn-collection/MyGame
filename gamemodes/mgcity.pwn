			}
		}
		else SendClientMessage(playerid, COLOR_GREY, "   Вы не Полицейский / Солдат !");
	}
	else SendClientMessage(playerid, COLOR_WHITE, "Подсказка: /camera [number 1 - 6]  (6 = OFF)");
	return 1;
}
