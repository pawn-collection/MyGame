CMD:scrap(playerid, params[])
{
	new string[32];
	if(IsPlayerInRangeOfPoint(playerid,6.0,-1866.5177,-1683.6747,21.3368))
	{
	    if(GetPVarInt(playerid,"LichCarID") == GetPlayerVehicleID(playerid))
		{
		    format(string, sizeof(string), "cars/%s.ini", GetName(playerid));
			if(fexist(string))
			{
			    SendClientMessage(playerid, -1, "{FFFFFF}Вы сдали свою машину в утиль. Вот ваши $7000.");
			    Cars[GetPVarInt(playerid,"LichCarID")] = 0;
			    GetVehicleParamsEx(GetPVarInt(playerid,"LichCarID"), engine, lights, alarm, doors, bonnet, boot, objective);
              	SetVehicleParamsEx(GetPVarInt(playerid,"LichCarID"), false, false, false, false, false, false, objective);
				oGivePlayerMoney(playerid,7000);
        		VehInfo[GetPVarInt(playerid,"LichCarID")][vLights] = 0;
        		VehInfo[GetPVarInt(playerid,"LichCarID")][vEngine] = 0;
    			Delete3DTextLabel(Text3D:CarInfoLabel[playerid]);
				DestroyVehicle(GetPVarInt(playerid,"LichCarID"));
			    format(string, sizeof(string), "cars/%s.ini", GetName(playerid));
                fremove(string);
			    SetPVarInt(playerid,"LichCarID",9999);
    		}
		}
		else SendClientMessage(playerid, COLOR_GRAD2, "Вы должны быть в своей машине.");
	}
	else SendClientMessage(playerid, COLOR_GRAD2, "Вы не в месте здачи авто.");
	return 1;
}
