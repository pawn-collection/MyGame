CMD:santa(playerid, params[])
{
	if (GolovnoyUbor[playerid] == 0)
	{
 		if(PlayerInfo[playerid][pSanta] == 1)
		{
  			SendClientMessage(playerid, COLOR_LIC, "{00FF36}** {EEE600}Вы одели Шапку Санты {00FF36} **");
