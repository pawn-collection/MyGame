public OnPlayerDisconnect(playerid, reason)
{
	gActivePlayers[playerid]--;
	ACResetPlayerWeapons(playerid);
	PlayerInfo[playerid][pAdjustable] = 1;
    GF_OnPlayerUpdate(playerid);
    DestroyObject( Player[playerid][camobj] );
    if(GetPVarInt(playerid,"LichCarID") != 9999)
    {
        UpdateCar(playerid);
        GetVehicleParamsEx(GetPVarInt(playerid,"LichCarID"), engine, lights, alarm, doors, bonnet, boot, objective);
    	SetVehicleParamsEx(GetPVarInt(playerid,"LichCarID"), false, false, false, false, false, false, objective);
    	Cars[GetPVarInt(playerid,"LichCarID")] = 0;
        Delete3DTextLabel(Text3D:CarInfoLabel[playerid]);
		DestroyVehicle(GetPVarInt(playerid,"LichCarID"));
	}
	if(GetPVarInt(playerid, "RENTCAR") != 9999)
	{
		UnLockCar(GetPVarInt(playerid, "RENTCAR"));
		SetPVarInt(playerid, "RENTCAR",9999);
	}
	if(velik[playerid] != 0)
	{
	    DestroyVehicle(velik[playerid]);
	}
	if(prosport[playerid] != 0)
	{
	    DestroyVehicle(prosport[playerid]);
	}
	foreach (Player, i)
	{
	    if(GetPlayerState(i) == PLAYER_STATE_SPECTATING && gSpectateID[i] == playerid)
		{
	       AdvanceSpectate(i);
		}
 		if(TaxiAccepted[i] < 999)
   		{
	        if(TaxiAccepted[i] == playerid)
	        {
	            TaxiAccepted[i] = 999;
	            GameTextForPlayer(i, "~w~ЏAKC… AЂOмEмЏ~n~~r~ЊOK…мУ‡ …‚PУ", 5000, 1);
	            TaxiCallTime[i] = 0;
	            DisablePlayerCheckpoint(i);
	        }
        }
		else if(BusAccepted[i] < 999)
       	{
  			if(BusAccepted[i] == playerid)
   			{
      			BusAccepted[i] = 999;
		        GameTextForPlayer(i, "~w~Bus AЂOмEмЏ~n~~r~ЊOK…мУ‡ …‚PУ", 5000, 1);
       			BusCallTime[i] = 0;
          		DisablePlayerCheckpoint(i);
	        }
      	}
	}
	if(GotHit[playerid] > 0)
	{
	    if(GetChased[playerid] < 999)
	    {
	       	SendClientMessage(GetChased[playerid], -1, "{FFFFFF}Ваш Заказ покинул сервер.");
	        GoChase[GetChased[playerid]] = 999;
	    }
	}
	if(GetPVarInt(playerid, "CoronosCar") != 0)
	{
	    DestroyVehicle(GetPVarInt(playerid, "CoronosCar"));
	    Cars[GetPVarInt(playerid, "CoronosCar")] = 0;
	}
	if(GetPVarInt(playerid, "StreetCar") != 0)
	{
	    DestroyVehicle(GetPVarInt(playerid, "StreetCar"));
	    Cars[GetPVarInt(playerid, "StreetCar")] = 0;
	}
	if(GetPVarInt(playerid, "copautos") != 0)
	{
	    DestroyVehicle(GetPVarInt(playerid, "copautos"));
	    Cars[GetPVarInt(playerid, "copautos")] = 0;
	}
	/*if(coroncar[playerid] != 0)
	{
	    DestroyVehicle(coroncar[playerid]);
	    VehInfo[coroncar[playerid]][vCarEC] = 0;
	}*/
	/*if(StreetCar[playerid] != 0)
	{
	    DestroyVehicle(StreetCar[playerid]);
	    VehInfo[StreetCar[playerid]][vCarSR] = 0;
	}*/
	if(PlayerPaintballing[playerid] != 0) PaintballPlayers --;
	if(PlayerKarting[playerid] > 0 && PlayerInKart[playerid] > 0) KartingPlayers --;
    if(TransportDuty[playerid] == 1) TaxiDrivers -= 1;
    else if(TransportDuty[playerid] == 2) BusDrivers -= 1;
	else if(TransportDuty[playerid] == 3) TramDrivers -= 1;
	else if(TransportDuty[playerid] == 4) BuzzDrivers -= 1;
	if(PlayerInfo[playerid][pJob] == 11)
	{
