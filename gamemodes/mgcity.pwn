CMD:tv(playerid, params[])
{
	new specid;
	if (!sscanf(params, "i", specid))
 	{
    	if (PlayerInfo[playerid][pMember] == 8 || PlayerInfo[playerid][pLeader] == 8)
        {
            if (IsPlayerInRangeOfPoint(playerid,2.0, 2546.8464,-1285.8193,1060.9844))
            {
                if(!IsPlayerConnected(specid)) return SendClientMessage(playerid, COLOR_RED, "Игрок оффлайн");
        		if(specid == playerid) return SendClientMessage(playerid, COLOR_RED, "Нельзя следить за собой.");
        		if(GetPlayerState(specid) == PLAYER_STATE_SPECTATING && gSpectateID[specid] != INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_RED, "Этот игрок сам следит за кем-то");
        		if (PlayerInfo[specid][pAdmin] >= 2000) return SendClientMessage(playerid, COLOR_RED, "Нельзя следить за Главным Админом");
        		if(GetPlayerState(specid) != 1 && GetPlayerState(specid) != 2 && GetPlayerState(specid) != 3) return SendClientMessage(playerid, COLOR_RED, "Игрок ещё не отреспавнился");
       		    StartSpectate(playerid, specid);
		   		return 1;
        	}
        	else
        	{
                if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING)
                {
                    StartSpectate(playerid, specid);
                }
                else SendClientMessage(playerid, COLOR_RED, "Вы не рядом с камерами наблюдения агенства.");
        	}
		}
	}
	else SendClientMessage(playerid, COLOR_RED, "Слежка: /tv [id]");
	return 1;
}
