		    string[i]=='Z' || string[i]=='X' || string[i]=='C' || string[i]=='V' ||
		    string[i]=='B' || string[i]=='N' || string[i]=='M') { } else return 0;
	   	}
	   	else if(i != next)
	   	{
		    if(string[i]=='q' || string[i]=='w' || string[i]=='e' ||
		    string[i]=='r' || string[i]=='t' || string[i]=='y' || string[i]=='u' ||
