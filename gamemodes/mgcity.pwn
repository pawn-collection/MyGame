       		RemovePlayerAttachedObject(playerid,3);
         	RemovePlayerAttachedObject(playerid,-1);
          	SetPlayerAttachedObject(playerid, 3, 19065, 2, 0.120000, 0.040000, -0.003500, 0, 100, 100, 1.4, 1.4, 1.4);
